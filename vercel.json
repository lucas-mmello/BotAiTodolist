{
  "version": 2,
  "build": {
    "env": {
      "NODE_VERSION": "20.x"
    }
  },
  "builds": [
    {
      "src": "api/gerar-tarefas.ts",
      "use": "@vercel/node@3.2.1"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/gerar-tarefas.ts",
      "methods": ["POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
